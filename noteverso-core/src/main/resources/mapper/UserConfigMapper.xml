<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.noteverso.core.dao.UserConfigMapper">
    <select id="findUserConfigByUserId" resultType="com.noteverso.core.model.UserConfig">
        SELECT * from noteverso_user_config
        WHERE user_id = #{userId}
    </select>
</mapper>
