<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.noteverso.core.dao.ProjectMapper">
    <select id="selectByProjectId" resultType="com.noteverso.core.model.Project">
        SELECT * FROM noteverso_project
        WHERE project_id = #{projectId} AND creator = #{userId}
    </select>
</mapper>
